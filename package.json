{
  "name": "@ugrab/k8s-dashboard",
  "version": "0.0.0-development",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "libs/*",
    "plugins/*"
  ],
  "scripts": {
    "dev": "nx run-many -t serve",
    "dev:server": "nx run server:serve",
    "plugin:docker:g": "rm -rf plugins/plugin-docker && nx g @k8sd/plugin-builder:plugin plugin-docker --scope=k8sd",
    "prisma:migrate": "yarn workspace @k8sd/server prisma:migrate",
    "prisma:serve": "yarn workspace @k8sd/server prisma:serve"
  },
  "dependencies": {
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@nx/nest": "16.6.0",
    "@nx/plugin": "16.6.0",
    "@nx/react": "16.6.0",
    "@swc/cli": "0.1.62",
    "esbuild": "0.19.1",
    "nx": "16.6.0",
    "prettier": "2.8.8",
    "prettier-plugin-packagejson": "2.4.5",
    "prettier-plugin-tsconfig": "0.0.1"
  },
  "packageManager": "yarn@3.6.1"
}
